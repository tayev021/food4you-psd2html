{
  "name": "food4you-markup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "live-server",
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "webserver": "live-server",
    "dev": "npm-run-all --parallel watch:sass webserver",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 5 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Taranov Eugene",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "live-server": "^1.2.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.7",
    "postcss-cli": "^8.3.1"
  }
}
